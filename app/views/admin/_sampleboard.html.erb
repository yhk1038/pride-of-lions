<div class="well well-out">
    <div class="well well-in">
        <table class="table" style="border-color: black;">
            <thead>
                <tr class="tr-head">
                    <td>#</td>
                    <td>이름</td>
                    <td>제목</td>
                </tr>
            </thead>
            <tbody>
                <% AdminHome.all.reverse.each do |post| %>
                    <tr class="tr-content">
                        <td><%= post.id %></td>
                        <td><%= post.board_name %></td>
                        <td><a href="/admin/index?admin=m_sb&id=<%= post.id %>&mod=1"><%= post.board_title %></a></td>
                    </tr>
                <% end %>
        </table>
    </div>

    <div class="row" align="right">
        
        <a class="btn btn-orange" href="/admin/index?admin=m_sb">등록</a>
    
    </div>


</div>
